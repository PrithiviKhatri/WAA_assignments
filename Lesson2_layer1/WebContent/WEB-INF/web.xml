    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>logIn</servlet-name>
    <servlet-class>mum.login</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>cofeechoice</servlet-name>
    <servlet-class>mum.Cofeechoice</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>cofeechoice</servlet-name>
    <url-pattern>/cofeechoice</url-pattern>
  </servlet-mapping>
</web-app>